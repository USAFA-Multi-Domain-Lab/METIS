  "scripts": {
    "start": "METIS_ENV_TYPE=prod tsx start.ts",
    "dev": "METIS_ENV_TYPE=dev tsx watch start.ts",
    "test": "export METIS_ENV_TYPE=test && jest",
    "test:api": "export METIS_ENV_TYPE=test && jest --selectProjects api-integration",
    "test:unit": "export METIS_ENV_TYPE=test && jest --selectProjects unit",
