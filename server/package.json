    "restore-users": "./node_modules/.bin/ts-node -r tsconfig-paths/register database/scripts/metis-users/manage-users --restore",
    "delete-users": "./node_modules/.bin/ts-node -r tsconfig-paths/register database/scripts/metis-users/manage-users --delete",
    "archive-users": "./node_modules/.bin/ts-node -r tsconfig-paths/register database/scripts/metis-users/manage-users --archive"
