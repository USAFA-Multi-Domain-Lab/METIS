{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "metis/target-environments": ["../../shared/target-environments"],
      "metis/target-environments/*": ["../../shared/target-environments/*"],
      "metis/toolbox/ObjectToolbox": [
        "../../shared/toolbox/objects/ObjectToolbox"
      ],
      "metis/toolbox/numbers/NumberToolbox": [
        "../../shared/toolbox/numbers/NumberToolbox"
      ],
      "metis/toolbox/VersionToolbox": [
        "../../shared/toolbox/strings/VersionToolbox"
      ],
      "metis/toolbox/BooleanToolbox": [
        "../../shared/toolbox/booleans/BooleanToolbox"
      ]
    },
    "module": "NodeNext",
    "moduleResolution": "NodeNext"
  },
  "include": [
    "./**/*",
    "../../shared/target-environments/*",
    "../../shared/toolbox/objects/ObjectToolbox.ts",
    "../../shared/toolbox/numbers/NumberToolbox.ts",
    "../../shared/toolbox/strings/VersionToolbox.ts",
    "../../shared/toolbox/booleans/BooleanToolbox.ts"
  ]
}
